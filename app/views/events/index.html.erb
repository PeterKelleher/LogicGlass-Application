<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Event date</th>
      <th>Event type</th>
      <th>Event status</th>
      <th>Sponsor</th>
      <th>Fee</th>
      <th>Category</th>
      <th>Address</th>
      <th>Building name</th>
      <th>City</th>
      <th>Zip</th>
      <th>State</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.description %></td>
        <td><%= event.event_date %></td>
        <td><%= event.event_type_id %></td>
        <td><%= event.event_status_id %></td>
        <td><%= event.sponsor_id %></td>
        <td><%= event.fee %></td>
        <td><%= event.category %></td>
        <td><%= event.address %></td>
        <td><%= event.building_name %></td>
        <td><%= event.city %></td>
        <td><%= event.zip %></td>
        <td><%= event.state_id %></td>
        <td><%= event.country_id %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
