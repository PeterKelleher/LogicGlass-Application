<p id="notice"><%= notice %></p>

<h1>Coordinator Check Ins</h1><br>

<section class="">
<div class="wrap-table100">
<div class="table100">
<table>
  <thead>
    <tr class="table100-head">
      <th class="column1">Coordinator</th>
      <th class="column2">Event</th>
      <th class="column3">Check in date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coordinator_check_ins.each do |coordinator_check_in| %>
      <tr>
        <td class="column1"><%= coordinator_check_in.coordinator.full_name %></td>
        <td class="column2"><%= coordinator_check_in.event.description %></td>
        <td class="column3"><%= coordinator_check_in.check_in_date.strftime("%I:%M %p") %></td>
        <td class="column4"><%= link_to 'View', coordinator_check_in, class: "show" %></td>
        <td class="column5"><%= link_to 'Edit', edit_coordinator_check_in_path(coordinator_check_in) %></td>
        <td class="column6"><%= link_to 'Destroy', coordinator_check_in, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Coordinator Check In', new_coordinator_check_in_path, class: "add" %>
</div>
</div>
</section>
