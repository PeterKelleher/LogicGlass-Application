<p id="notice"><%= notice %></p>

<h1>Coordinators</h1>

<table>
  <thead>
    <tr>
      <th>Peoplesoft</th>
      <th>Position</th>
      <th>First name</th>
      <th>Middle initial</th>
      <th>Last name</th>
      <th>Date of birth</th>
      <th>Address</th>
      <th>City</th>
      <th>Zip</th>
      <th>State</th>
      <th>Country</th>
      <th>Home phone</th>
      <th>Mobile phone</th>
      <th>Payment</th>
      <th>Student class</th>
      <th>Major</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coordinators.each do |coordinator| %>
      <tr>
        <td><%= coordinator.peoplesoft_id %></td>
        <td><%= coordinator.position %></td>
        <td><%= coordinator.first_name %></td>
        <td><%= coordinator.middle_initial %></td>
        <td><%= coordinator.last_name %></td>
        <td><%= coordinator.date_of_birth %></td>
        <td><%= coordinator.address %></td>
        <td><%= coordinator.city %></td>
        <td><%= coordinator.zip %></td>
        <td><%= coordinator.state_id %></td>
        <td><%= coordinator.country_id %></td>
        <td><%= coordinator.home_phone %></td>
        <td><%= coordinator.mobile_phone %></td>
        <td><%= coordinator.payment %></td>
        <td><%= coordinator.student_class_id %></td>
        <td><%= coordinator.major %></td>
        <td><%= link_to 'Show', coordinator %></td>
        <td><%= link_to 'Edit', edit_coordinator_path(coordinator) %></td>
        <td><%= link_to 'Destroy', coordinator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coordinator', new_coordinator_path %>
